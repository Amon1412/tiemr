<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_complete"
    tools:context=".activity.TimerCompleteActivity">
    <ImageView
        android:id="@+id/ic_complete"
        android:layout_width="@dimen/dp_43"
        android:layout_height="@dimen/dp_43"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="@dimen/dp_19"
        android:layout_marginLeft="@dimen/dp_18"
        android:src="@drawable/ic_complete"/>
    <ImageView
        android:id="@+id/ic_restart"
        android:layout_width="@dimen/dp_43"
        android:layout_height="@dimen/dp_43"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="@dimen/dp_19"
        android:layout_marginRight="@dimen/dp_13"
        android:clickable="true"
        android:background="@drawable/ic_restart"/>

    <com.sgtc.countdown.view.Timer
        android:id="@+id/timer"
        android:layout_width="@dimen/dp_204"
        android:layout_height="@dimen/dp_204"
        android:layout_centerInParent="true" />
</RelativeLayout>
